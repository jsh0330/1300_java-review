package MyException;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int num1 = 0, num2 = 0, num3 = 0;
		
		System.out.print("나누어질 숫자를 입력하세요 : ");
		num1 = sc.nextInt();
		System.out.println();
		
		System.out.print("나눌 숫자를 입력하세요 : ");
		num2 = sc.nextInt();
		System.out.println();
	}

}
